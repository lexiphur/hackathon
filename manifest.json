{
  "manifest_version": 2,
  "name": "PP Links Helper",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://stackoverflow.com/*"],
      "js": ["./main.js"]
    }
  ],
  "icons": {
    "100": "daveFace.png"
},
  "browser_action": {
      "default_popup": "popup.html"

  }
  
}